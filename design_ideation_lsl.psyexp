        <Param name="Begin Experiment" updates="constant" val="num_one_back = 20&amp;#10;letters = ['A', 'B', 'C', 'D', 'E', 'F', 'G', 'H', 'I', 'J', 'K', 'L']" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="num_one_back = 20;&amp;#10;letters = [&quot;A&quot;, &quot;B&quot;, &quot;C&quot;, &quot;D&quot;, &quot;E&quot;, &quot;F&quot;, &quot;G&quot;, &quot;H&quot;, &quot;I&quot;, &quot;J&quot;, &quot;K&quot;, &quot;L&quot;];&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="outlet.push_sample([&quot;One-back initialize&quot;, (design_problems.thisN + 1).toString(), stimuli.toString(), &quot;&quot;, &quot;&quot;, &quot;&quot;]);&amp;#10;correct = 0;&amp;#10;total = 0;&amp;#10;p = 0.4;&amp;#10;bool_mask = randchoice([true, false], {&quot;size&quot;: num_one_back, &quot;p&quot;: [p, (1 - p)]});&amp;#10;first_letter = randchoice(letters);&amp;#10;one_back_stim = [];&amp;#10;for (var i, _pj_c = 0, _pj_a = util.range(num_one_back), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    i = _pj_a[_pj_c];&amp;#10;    prev_stim = ((i === 0) ? first_letter : one_back_stim[(i - 1)]);&amp;#10;    if (bool_mask[i]) {&amp;#10;        one_back_stim.push(prev_stim);&amp;#10;    } else {&amp;#10;        new_stim = randchoice(function () {&amp;#10;    var _pj_d = [], _pj_e = letters;&amp;#10;    for (var _pj_f = 0, _pj_g = _pj_e.length; (_pj_f &lt; _pj_g); _pj_f += 1) {&amp;#10;        var x = _pj_e[_pj_f];&amp;#10;        if ((x !== prev_stim)) {&amp;#10;            _pj_d.push(x);&amp;#10;        }&amp;#10;    }&amp;#10;    return _pj_d;&amp;#10;}&amp;#10;.call(this));&amp;#10;        one_back_stim.push(new_stim);&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="outlet.push_sample(['One-back initialize', str(design_problems.thisN + 1), str(stimuli),'', '', ''])&amp;#10;&amp;#10;# Init. counters to zero&amp;#10;correct = 0&amp;#10;total = 0&amp;#10;&amp;#10;# Init. letters for N-back&amp;#10;p = 0.4 # Prob. of &quot;true&quot; stim&amp;#10;bool_mask = randchoice([True, False], size=(num_one_back),p=[p,1-p])&amp;#10;&amp;#10;first_letter = randchoice(letters)&amp;#10;one_back_stim = []&amp;#10;&amp;#10;for i in range(num_one_back): &amp;#10;    prev_stim = first_letter if i == 0 else one_back_stim[i-1]&amp;#10;    &amp;#10;    if bool_mask[i]:    &amp;#10;        one_back_stim.append(prev_stim)&amp;#10;    else:&amp;#10;        new_stim = randchoice([x for x in letters if x!= prev_stim])&amp;#10;        one_back_stim.append(new_stim)&amp;#10;&amp;#10;&amp;#10;#print(first_letter)        &amp;#10;#print(bool_mask)&amp;#10;#print(one_back_stim)&amp;#10;&amp;#10;&amp;#10;" valType="extendedCode"/>
